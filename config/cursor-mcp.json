{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@smithery/cli", "run", "@smithery-ai/server-sequential-thinking"],
      "config": {}
    },
    "tavily": {
      "command": "python3",
      "args": ["services/tavily/mcp2tavily.py"],
      "env": {
        "TAVILY_API_KEY": "your_api_key_here",
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    "obsidian": {
      "command": "node",
      "args": ["services/obsidian/obsidian_mcp.js"],
      "env": {
        "OBSIDIAN_API_KEY": "558d634153bc2cea74263c5b37c47a7fe1a5bbe523d20b5ea646e053f0c59e31"
      }
    }
  }
}